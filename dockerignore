# Ignore version control directories
.git/
.svn/
.hg/

# Ignore dependency directories (language-specific)
node_modules/
vendor/
__pycache__/
target/

# Ignore IDE/editor files
.idea/
.vscode/
*.swp
*.suo

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore logs and temporary files
*.log
*.tmp
*.bak

# Ignore Docker-related files
<!-- Dockerfile -->
.dockerignore
<!-- compose.yaml -->
<!-- *.docker.* -->

# Ignore OS-specific files
Thumbs.db
.DS_Store

# Ignore large or sensitive files
*.zip
*.tar.gz
*.pem
*.key